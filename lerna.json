{
  "ci": false,
  "command": {
    "exec": {
      "ignore": "@appium/eslint-config-appium"
    },
    "publish": {
      "distTag": "next",
      "npmClientArgs": [
        "--dry-run"
      ],
      "preid": "beta"
    },
    "version": {
      "push": false,
      "conventionalCommits": true
    }
  },
  "ignoreChanges": [
    "**/test/**",
    "**/*.md"
  ],
  "npmClientArgs": [
    "--no-package-lock"
  ],
  "packages": [
    "packages/*"
  ],
  "version": "independent"
}
